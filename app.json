{
  "expo": {
    "name": "こころのリビング",
    "slug": "kokoro-living",
    "version": "1.2.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kokoroliving",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f0f0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kokoroliving.app",
      "buildNumber": "30",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDevelopmentRegion": "ja",
        "CFBundleLocalizations": ["ja", "en"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 30,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.READ_MEDIA_IMAGES"
      ],
      "blockedPermissions": [
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kokoroliving.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "kokoroliving",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "プロフィール画像を選択するために写真ライブラリへのアクセスが必要です。"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "AI振り返り結果を画像として保存するために写真ライブラリへのアクセスが必要です。",
          "savePhotosPermission": "AI振り返り結果を画像として保存するために写真ライブラリへのアクセスが必要です。",
          "isAccessMediaLocationEnabled": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dd6d9f51-3b91-4e88-bb2c-ce81ab2da5c0"
      }
    },
    "owner": "yutatrm"
  }
}
